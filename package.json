{
  "name": "nazzel-messenger-userbot",
  "version": "2.2.1",
  "description": "Advanced Facebook Messenger User-Bot with TypeScript, MongoDB, and comprehensive logging",
  "main": "dist/main.js",
  "type": "module",
  "packageManager": "pnpm@10.24.0",
  "scripts": {
    "start": "tsx src/main.ts",
    "dev": "tsx watch src/main.ts",
    "build": "tsc",
    "lint": "eslint src/",
    "format": "prettier --write src/"
  },
  "pnpm": {
    "overrides": {
      "tough-cookie": "^5.1.2"
    }
  },
  "keywords": [
    "messenger",
    "userbot",
    "facebook",
    "typescript",
    "chatbot",
    "mongodb"
  ],
  "author": "Nazzel",
  "license": "SEE LICENSE IN LICENSE.md",
  "dependencies": {
    "@dongdev/fca-unofficial": "^3.0.8",
    "@spotify/web-api-ts-sdk": "^1.2.0",
    "chalk": "^5.6.2",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "ioredis": "^5.8.2",
    "mongodb": "^7.0.0",
    "node-fetch": "^3.3.2",
    "openai": "^6.10.0",
    "uuid": "^11.1.0",
    "winston": "^3.18.0",
    "ytdl-core": "^4.11.5"
  },
  "devDependencies": {
    "@types/express": "^5.0.1",
    "@types/node": "^22.15.21",
    "@types/uuid": "^10.0.0",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
